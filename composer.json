{
    "name": "votre-projet/nom",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "siovision/sio_sso_lib": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/siovision/sio_sso_lib"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Siovision\\SioSsoLib\\": "vendor/siovision/sio_sso_lib/src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}